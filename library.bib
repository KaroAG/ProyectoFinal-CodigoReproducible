issn = {20734425},
journal = {Genes},
keywords = {Aneuploidy,Autokaryogamy,Cancer,Chromothripsis,Cleavage embryo,Disabled spindle,Meio-mitosis,Recombination on kinetochores,Reduction,Somatic pairing},
number = {2},
pages = {1--21},
