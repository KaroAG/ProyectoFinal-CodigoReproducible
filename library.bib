file = {:C$\backslash$:/Users/Karol-PC/AppData/Local/Mendeley Ltd./Mendeley Desktop/Downloaded/Luo et al. - 2019 - c-Myc inhibits myoblast differentiation and promotes myoblast proliferation and muscle fibre hypertrophy by regulati.pdf:pdf},
isbn = {4141801801},
issn = {14765403},
journal = {Cell Death and Differentiation},
number = {3},
pages = {426--442},
publisher = {Springer US},
title = {{c-Myc inhibits myoblast differentiation and promotes myoblast proliferation and muscle fibre hypertrophy by regulating the expression of its target genes, miRNAs and lincRNAs}},
